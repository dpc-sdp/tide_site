{
    "name": "dpc-sdp/tide_site",
    "description": "Post content to multiple sites and sections.",
    "type": "drupal-module",
    "license": "GPL-2.0+",
    "require": {
        "dpc-sdp/tide_core": "dev-feature/SDPA-940-Add-aditional-local-sub-footer",
        "dpc-sdp/tide_media": "@dev"
    },
    "require-dev": {
        "dpc-sdp/tide_test": "@dev"
